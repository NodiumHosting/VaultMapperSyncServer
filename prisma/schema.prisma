generator client {
    provider = "prisma-client-js"
    output   = "../generated/client"
}

datasource db {
    provider = "sqlite"
    url      = "file:../vmsync.db"
}

model Player {
    playerUuid String   @id @unique
    color      String
    createdAt  DateTime @default(now())
}

model Stats {
    stat  String @id @unique
    value Int
}
